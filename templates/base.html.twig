<!doctype html>
<html lang="en">
	<head>
		<!-- Required meta tags -->
		<meta charset="utf-8">
		<meta
		name="viewport" content="width=device-width, initial-scale=1">

		<!-- foot  -->
		<link
		rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg==" crossorigin="anonymous" referrerpolicy="no-referrer"/>
		<!-- Bootstrap CSS -->

		<link
		href="/css/bootstrap.min.css" rel="stylesheet">
		<!-- Custom CSS   -->
		<link rel="shortcut icon" href="{{ asset('upload/logo1.svg') }}" type="image/x-icon"> {% block stylesheets %}{% endblock %}
		<style>
			* {
				margin: 0;
				padding: 0;
				box-sizing: border-box;
			}
			body {
				display: flex;
				flex-direction: column;
				min-height: 100vh;
			}
		</style>
		{{ encore_entry_script_tags('app') }}
		{{ encore_entry_link_tags('app') }}
		<title>
			{% block title %}{% endblock %}
		</title>
	</head>
	<body>
		{% include 'Partial/_navBar.twig' %}
		{% block body %}{% endblock %}
		{% include 'Partial/_footerBar.twig' %}
		<!-- Bootstrap JS -->
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
		{% block javascripts %}{% endblock %}


	</body>
</html>
